<h2>오가게</h2>
<div class="row" ng-controller="ChartCtrlGA">
    <input type="hidden" ng-model="domCd" value="GA" />
    <div class="panel-body">
        <select ng-model="beforeDay" ng-change="getDdlSearchChart(beforeDay);">
